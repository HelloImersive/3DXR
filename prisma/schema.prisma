generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("PRISMA_DATABASE_URL")
}

model Session {
  id          String    @id
  shop        String
  state       String
  isOnline    Boolean?  @default(false)
  scope       String?
  expires     DateTime? @db.Timestamp(6)
  accessToken String
  userId      BigInt?
}
